{
  "name": "mobile",
  "$schema": "../../node_modules/nx/schemas/project-schema.json",
  "sourceRoot": "apps/mobile/src",
  "projectType": "application",
  "targets": {
    "start": {
      "executor": "@nx/expo:start",
      "dependsOn": ["ensure-symlink", "sync-deps"],
      "options": {
        "port": 8081
      }
    },
    "serve": {
      "executor": "nx:run-commands",
      "options": {
        "command": "nx start mobile"
      }
    },
    "run-ios-dev": {
      "executor": "@nx/expo:run",
      "dependsOn": ["mount-env-dev", "ensure-symlink", "sync-deps"],
      "options": {
        "platform": "ios"
      }
    },
    "run-ios-qa": {
      "executor": "@nx/expo:run",
      "dependsOn": ["mount-env-qa", "ensure-symlink", "sync-deps"],
      "options": {
        "platform": "ios"
      }
    },
    "run-ios-prod": {
      "executor": "@nx/expo:run",
      "dependsOn": ["mount-env-prod", "ensure-symlink", "sync-deps"],
      "options": {
        "platform": "ios",
        "clean": true
      }
    },
    "run-android-dev": {
      "executor": "@nx/expo:run",
      "dependsOn": ["mount-env-dev", "ensure-symlink", "sync-deps"],
      "options": {
        "platform": "android",
        "variant": "debug",
        "clean": true
      }
    },
    "run-android-qa": {
      "executor": "@nx/expo:run",
      "dependsOn": ["mount-env-qa", "ensure-symlink", "sync-deps"],
      "options": {
        "platform": "android",
        "variant": "release",
        "clean": true
      }
    },
    "run-android-prod": {
      "executor": "@nx/expo:run",
      "dependsOn": ["mount-env-prod", "ensure-symlink", "sync-deps"],
      "options": {
        "platform": "android",
        "variant": "release",
        "clean": true
      }
    },
    "build": {
      "executor": "@nx/expo:build",
      "options": {}
    },
    "eas-build-android-production": {
      "executor": "@nx/expo:build",
      "options": {
        "platform": "android",
        "json": true,
        "profile": "production",
        "interactive": false,
        "local": false,
        "wait": true,
        "clearCache": true,
        "autoSubmitWithProfile": "production"
      }
    },
    "eas-build-android-dev": {
      "executor": "@nx/expo:build",
      "options": {
        "platform": "android",
        "json": true,
        "profile": "development",
        "interactive": false,
        "wait": true,
        "clearCache": true
      }
    },
    "submit": {
      "executor": "@nx/expo:submit",
      "options": {}
    },
    "build-list": {
      "executor": "@nx/expo:build-list",
      "options": {}
    },
    "download": {
      "executor": "@nx/expo:download",
      "options": {
        "output": "apps/mobile/dist"
      }
    },
    "sync-deps": {
      "executor": "@nx/expo:sync-deps",
      "options": {}
    },
    "ensure-symlink": {
      "executor": "@nx/expo:ensure-symlink",
      "options": {}
    },
    "prebuild": {
      "executor": "@nx/expo:prebuild",
      "dependsOn": ["ensure-symlink", "sync-deps"],
      "options": {}
    },
    "install": {
      "executor": "@nx/expo:install",
      "options": {}
    },
    "update": {
      "executor": "@nx/expo:update",
      "options": {}
    },
    "export": {
      "executor": "@nx/expo:export",
      "dependsOn": ["ensure-symlink", "sync-deps"],
      "options": {
        "platform": "all",
        "outputDir": "../../dist/apps/mobile"
      }
    },
    "export-web": {
      "executor": "@nx/expo:export",
      "options": {
        "bundler": "metro"
      }
    },
    "lint": {
      "executor": "@nx/linter:eslint",
      "outputs": ["{options.outputFile}"],
      "options": {
        "lintFilePatterns": ["apps/mobile/**/*.{ts,tsx,js,jsx}"]
      }
    },
    "test": {
      "executor": "@nx/jest:jest",
      "outputs": ["{workspaceRoot}/coverage/{projectRoot}"],
      "options": {
        "jestConfig": "apps/mobile/jest.config.ts",
        "passWithNoTests": true
      },
      "configurations": {
        "ci": {
          "ci": true,
          "codeCoverage": true
        }
      }
    },
    "clean-gradle": {
      "executor": "nx:run-commands",
      "options": {
        "command": "cd apps/mobile/android && ./gradlew clean && cd ../../.."
      }
    },
    "mount-env-dev": {
      "executor": "nx:run-commands",
      "options": {
        "command": "cd apps/mobile/env && cp .env.dev .env && cd ../../.."
      }
    },
    "mount-env-prod": {
      "executor": "nx:run-commands",
      "options": {
        "command": "cd apps/mobile/env && cp .env.prod .env && cd ../../.."
      }
    },
    "mount-env-qa": {
      "executor": "nx:run-commands",
      "options": {
        "command": "cd apps/mobile/env && cp .env.qa .env && cd ../../.."
      }
    }
  },
  "tags": []
}
